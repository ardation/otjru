<% content_for :head do %>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<meta name="viewport" content="user-scalable=no, width=device-width, initial-scale=1.0, maximum-scale=1.0"/>
	<meta name="apple-mobile-web-app-capable" content="yes" />

	<meta name="apple-mobile-web-app-status-bar-style" content="black" />
	<link rel="apple-touch-icon" href="<%= asset_path 'logo.png' %>"/>
	<link rel="icon" type="image/png" href="<%= asset_path 'favicon.png' %>">

	<title><%=t 'misc.otj' %> - <%= @outreach.org_name %></title>

	<script type="text/javascript">
		var I18n = {};
		I18n.locale = "<%= I18n.locale %>";
		I18n.step6_error_fname = "<%=t 'details.name.first_name.error' %>";
		I18n.step6_error_lname = "<%=t :'details.name.last_name.error' %>";
		I18n.step6_error_gender = "<%=t 'details.gender.error' %>";
		I18n.step6_error_mobile = "<%=t 'details.mobile.error' %>";
    I18n.step6_error_email = "<%=t 'details.email.error' %>";
    I18n.step6_error_university = "<%=t 'details.university.error' %>";
    I18n.step6_error_faculty = "<%=t 'details.faculty.error' %>";
		I18n.step6_error_year = "<%=t 'details.year.error' %>";
	</script>
	<%= stylesheet_link_tag "style" %>
	<%= stylesheet_link_tag "style.#{I18n.locale}" %>

<% end %>

<!-- Main container -->
<div id="container">
  <div id="upper-foreground">
    <div>
    </div>
  </div>
  <div role="main" id="main">
    <div>
      <div>
      </div>
    </div>
  </div>
  <div id="lower-foreground">
  </div>
  <div id="footer">
    <a href="step1" id="reset" class="clickable"><%=t 'misc.reset' %></a>
    <% unless @outreach.english_only? %><a href="/<%=t 'misc.switch_tag' %>" id="language" class="clickable" data-bypass><%=t 'misc.switch' %></a><% end %>
  </div>
</div>
<!-- Application source -->
<script src="http://maps.google.com/maps/api/js?libraries=places&sensor=true&language=<%= I18n.locale %>" type="text/javascript"></script>
<script src="<%= asset_path 'config.js' %>" type="text/javascript"></script>
