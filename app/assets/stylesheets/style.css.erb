  width:708px;
  height:200px;
}
#step8 .question {
  text-align: center;
}
#data {
  padding:20px;
}
#data .question {
  font-size:14px;
}
#data .label_check, #data .label_radio {
  font-size:14px;
  padding-bottom:0;
  padding-left:25px;
}
#data #locality {
  width:480px;
}
#data #table1, #data #table2 {
  float:none;
  width:100%;
}
#data .first {
  vertical-align:middle;
}
#data #fname {
  margin-bottom:5px;
}
.header {
  padding-top:10px;
}
#lname {
  max-width: 150px;
}
@media screen and (max-width:480px) {
  #lower-foreground {
    background-size:480px 60px;
    height:50px;
  }
  #step2 .next {
    bottom:55px !important;
  }
  #step2 .error {
    bottom:100px !important;
  }
}
@media screen and (max-width:800px) {
  .error {
    position:static;
    text-align:center;
    margin-right:10px;
    margin-left:10px;
    bottom:auto;
    height:15px;
    line-height:15px;
  }
  #step2 .error {
    position:absolute;
    bottom:250px;
    left:0;
    right:0;
    top:auto;
  }
  .error strong {
    display:none;
  }
  #step6 .error {
    margin-top:-7px;
  }
  #step6 td {
  vertical-align:middle;
  }
  .header {
    display:none;
  }
  .mobile {
    display:inline;
  }
  .mobile_block {
    display:block;
  }
  .non_mobile, .non_mobile_block, .non_mobile_tr {
    display:none;
  }
  label {
    font-size:15px;
    padding-bottom:5px;
  }
  .question {
    line-height:1.3;
    font-family: 'Museo-500', Arial;
    font-size:20px;
  }
  .question small {
    font-family: 'Museo-300', Arial;
    font-size:10px;
  }
  .question strong {
    font-family: 'Museo-700', Arial;
  }
  #step1 .question {
    display: block;
    background: url(<%= asset_path 'light_wool.png' %>) repeat;
    border-top:5px #fe57a1 solid;
    margin:10px;
    border-radius:0 0 5px 5px;
    text-align:center;
    padding:10px;
  }
  #step5 .question {
    font-size:13px;
    line-height:20px;
    margin-left:0;
  }
  #step1, #step3, #step4, #step5, #step6, #kennedy, #step8 {
    border:none;
    background:none;
    position:static;
    bottom:0;
    top:0;
    left:0;
    right:0;
    margin:0;
    padding:0;
    height:auto;
    width:auto;
  }
  #step1 .img, #step8 .img {
    position:static;
    background:url(<%= asset_path 'rocket.png' %>) top center no-repeat;
    background-size:90%;
    display: block;
    left:-20px;
    right:0;
    margin-top:20px;
    height:300px;
  }
  #step1 #start, #step2 .next, #step3 .next, #step4 .next, #step5 .next, #step6 .next, #step8 .next, #kennedy .next {
    padding:5px;
    background:#ff5c5c url(<%= asset_path 'right.png' %>) center right no-repeat;
    border-radius:5px;
    display: block;
    height:auto;
    width:auto;
    text-align:center;
    position:static;
    margin-right:10px;
    margin-left:10px;
    margin-top:10px;
    font-size:20px;
    float:none;
  }
  #step2 .next {
    position:absolute;
    bottom:200px;
    left:0;
    right:0;
    top:auto;
  }
  #step1 #start:hover, .next:hover {
    background:#fc5173 url(<%= asset_path 'right.png' %>) center right no-repeat !important;
  }
  #step2 .inputbox {
    background: url(<%= asset_path 'light_wool.png' %>) repeat;
    border-top:5px #21c6f4 solid;
    border-radius:0 0 5px 5px;
  }
  #magazine_border {
    display:none;
  }
  #step3 .option_position, #step4 .option_position, #step5 .option_position, #step6 .option_position, #step8 .option_position, #kennedy .option_position {
    margin:10px;
    background: url(<%= asset_path 'light_wool.png' %>) repeat;
    padding:10px;
    border-radius:0 0 5px 5px;
  }
  #step3 .option_position {
    border-top:5px #ffaa59 solid;
  }
  #step4 .option_position {
    border-top:5px #fb67a6 solid;
  }
  #step5 .option_position {
    border-top:5px #02ba7f solid;
  }
  #step6 .option_position {
    border-top:5px #7dd3cf solid;
  }
  #step8 .option_position {
    text-align:center;
    border-top:5px #bf0000 solid;
  }
  #kennedy .option_position {
    border-top:5px #eeb300 solid;
    min-height:0;
  }
  #kennedy .question {
    padding-bottom:0;
  }
  #kennedy .question small {
    font-size:11px;
  }
  #step4 .label_radio {
    font-size:14px;
  }
  #step6 .label_radio {
    padding-bottom:0;
  }
  #table1, #table2 {
    float:none;
  }
  #step5 label, #kennedy label {
    min-width:40px;
  }
  #step5 label strong {
    font-size:80%;
  }
  #step6 .question {
    font-size:15px;
  }
  #kennedy .question {
    font-size:15px;
  }
  #step6 .question small {
    font-size:12px;
  }
  #step6 td {
    padding-top:2px;
    vertical-align:middle;
  }
  #footer {
    left:5px;
    right:5px;
    bottom:5px;
  }
  .hide_small {
    display:none;
  }
  #step5 .iaac {
    margin-left:10px;
  }
  hr {
    margin: 3px 0;
  }
  #fname {
    margin-bottom:5px;
  }
  #step7 {
    position:static;
    width:100%;
    height:290px;
    margin:0;
  }
  #step7 .question {
    text-align:center;
  }
}
@media screen and (max-height:250px) {

  /*#lower-foreground {
    display:none;
  }
  #footer {
    display:none;
  }*/
   #step1 #start, #step2 .next, #step3 .next{
    bottom:10px;
  }
}
@media screen and (max-height:768px) {
  #step3, #step4, #step5, #step6, #step7, #step8, #kennedy {
    top:50px;
    margin-top:0;
  }
}

@media screen and (max-height:620px) {
  #lower-foreground {
    background:none;
    height:auto;
  }
  #step3, #step4, #step5, #step6, #step7, #step8, #kennedy {
    top: 20px;
    margin-top: 0;
  }
}
